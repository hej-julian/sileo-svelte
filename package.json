{
	"name": "sileo-svelte",
	"version": "0.1.3",
	"description": "An opinionated, physics-based toast notification library with a React-free Svelte entry.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hej-julian/sileo-svelte.git"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"default": "./dist/index.js"
		},
		"./svelte": {
			"import": {
				"types": "./dist/svelte.d.mts",
				"default": "./dist/svelte.mjs"
			},
			"require": {
				"types": "./dist/svelte.d.ts",
				"default": "./dist/svelte.js"
			},
			"default": "./dist/svelte.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\" && bunchee src/index.ts src/svelte.ts && node -e \"require('fs').copyFileSync('src/styles.css','dist/styles.css')\"",
		"dev": "bunchee --watch"
	},
	"peerDependencies": {
		"react": ">=18",
		"react-dom": ">=18"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		},
		"react-dom": {
			"optional": true
		}
	},
	"devDependencies": {
		"bunchee": "^6.9.4"
	},
	"dependencies": {
		"motion": "^12.34.0"
	}
}
